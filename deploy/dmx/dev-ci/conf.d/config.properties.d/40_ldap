dmx.ldap.protocol = LDAP
dmx.ldap.server = ldap
dmx.ldap.port = 389
#dmx.ldap.manager = cn=bind,dc=zukunftswerk,dc=org
#dmx.ldap.password = ${LDAP_READONLY_USER_PASSWORD}
dmx.ldap.manager = cn=admin,dc=zukunftswerk,dc=org
dmx.ldap.password = ${LDAP_ADMIN_PASSWORD}
dmx.ldap.user_base = ou=users,dc=zukunftswerk,dc=org
dmx.ldap.group_base = dc=zukunftswerk,dc=org
dmx.ldap.user_attribute = uid
dmx.ldap.user_filter = (&(objectclass=inetOrgPerson)(memberof=cn=zukunftswerk_users,dc=zukunftswerk,dc=org))
dmx.ldap.user_member_group = cn=zukunftswerk_users,dc=zukunftswerk,dc=org
dmx.ldap.logging = DEBUG
dmx.ldap.user_creation.enabled = true
